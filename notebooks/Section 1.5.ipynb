   "source": [
    "# Secant Method"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "f = x-> exp(x)-x-2\n",
    "\n",
    "n_iter=7\n",
    "\n",
    "x₀ = 2\n",
    "x₁ = 1\n",
    "\n",
    "\n",
    "println(\"iter \", 0,\" x = \", x₁, \" f(x) = \", f(x₁));\n",
    "for j = 1:n_iter\n",
    "    x = x₁ - f(x₁) * (x₁-x₀)/(f(x₁)-f(x₀))\n",
    "    x₀ = x₁\n",
    "    x₁ = x\n",
    "    println(\"iter \", j,\" x = \", x₁, \" f(x) = \", f(x₁));\n",
    "end\n"
   ]
  }
 ],
